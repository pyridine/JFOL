;;Remember, use csi -ss to run a chicken script.

;;Or, use the shebang #!/usr/local/bin/csi -script

(require 'posix)

(change-directory "../solver")

(load "Load-Prover.scm")

(define conclusion '())

(define premises '())

;;Conclusions
(define conclusion (cons (relation 'Hates (constant 'me) (constant 'me) ) conclusion))
;;Premises
(define premises (cons (universal 'X (binary 'IMP (relation 'Sucks (function 'life-of (variable 'X) )) (relation 'Hates (variable 'X) (variable 'X) ) )) premises))
(define premises (cons (relation 'Sucks (function 'life-of (constant 'me) )) premises))
(define argument (cons (car conclusion) premises))



(prove-argument argument)   

